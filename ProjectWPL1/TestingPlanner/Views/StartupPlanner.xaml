<Window x:Class="TestingPlanner.Views.StartupPlanner"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestingPlanner.Views"
        mc:Ignorable="d"
        Title="StartupPlanner"
        Height="720" Width="980" Background="#363636" Foreground="White">
    <StackPanel>
        <Image Style="{StaticResource BarcoLogo}" Width="158"/>
        <StackPanel Margin="10" Height="472">
            <GroupBox Header="Approval queue" BorderThickness="0.1" Height="359">
                <TabControl Background="#363636" Margin="0,23,0,-5">
                    <TabItem Header="EMC">
                        <Grid Background="#363636" Height="217">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="0" HorizontalAlignment="Center" Foreground="White" Text="DIVISION"/>
                            <TextBlock Grid.Column="1" HorizontalAlignment="Center" Foreground="White" Text="PROJECT NAME"/>
                            <TextBlock Grid.Column="2" HorizontalAlignment="Center" Foreground="White" Text="JR nr"/>
                            <TextBlock Grid.Column="3" HorizontalAlignment="Center" Foreground="White" Text="STATUS"/>
                            <TextBlock Grid.Column="4" HorizontalAlignment="Center" Foreground="White" Text="FORESEEN DATE"/>
                            <ListBox HorizontalAlignment="Center" Foreground="White" Name="lstDiv" Background="#363636" Grid.Column="0" BorderThickness="0"></ListBox>
                            <ListBox HorizontalAlignment="Center" Foreground="White" Name="lstProjectname" Background="#363636" Grid.Column="1" BorderThickness="0"></ListBox>
                            <ListBox HorizontalAlignment="Center" Foreground="White" Name="lstJrnr" Background="#363636" Grid.Column="2" BorderThickness="0"></ListBox>
                            <ListBox HorizontalAlignment="Center" Foreground="White" Name="lstStatus" Background="#363636" Grid.Column="3" BorderThickness="0"></ListBox>
                            <ListBox HorizontalAlignment="Center" Foreground="White" Name="lstForeseendate" Background="#363636" Grid.Column="4" BorderThickness="0"></ListBox>
                        </Grid>
                    </TabItem>
                    <TabItem Header="ENV">
                        <Grid Background="#363636" Height="217">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="0" HorizontalAlignment="Center" Foreground="White" Text="DIVISION"/>
                            <TextBlock Grid.Column="1" HorizontalAlignment="Center" Foreground="White" Text="PROJECT NAME"/>
                            <TextBlock Grid.Column="2" HorizontalAlignment="Center" Foreground="White" Text="JR nr"/>
                            <TextBlock Grid.Column="3" HorizontalAlignment="Center" Foreground="White" Text="STATUS"/>
                            <TextBlock Grid.Column="4" HorizontalAlignment="Center" Foreground="White" Text="FORESEEN DATE"/>
