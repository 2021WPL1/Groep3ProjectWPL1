                            <ListBox HorizontalAlignment="Center" Foreground="White" Name="lstSafjrnr" Background="#363636" Grid.Column="2" BorderThickness="0"></ListBox>
                            <ListBox HorizontalAlignment="Center" Foreground="White" Name="lstSafstatus" Background="#363636" Grid.Column="3" BorderThickness="0"></ListBox>
                            <ListBox HorizontalAlignment="Center" Foreground="White" Name="lstSafforeseendate" Background="#363636" Grid.Column="4" BorderThickness="0"></ListBox>
                        </Grid>
                    </TabItem>
                    <TabItem Header="PCK">
                        <Grid Background="#363636" Height="217">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="0" HorizontalAlignment="Center" Foreground="White" Text="DIVISION"/>
                            <TextBlock Grid.Column="1" HorizontalAlignment="Center" Foreground="White" Text="PROJECT NAME"/>
                            <TextBlock Grid.Column="2" HorizontalAlignment="Center" Foreground="White" Text="JR nr"/>
                            <TextBlock Grid.Column="3" HorizontalAlignment="Center" Foreground="White" Text="STATUS"/>
                            <TextBlock Grid.Column="4" HorizontalAlignment="Center" Foreground="White" Text="FORESEEN DATE"/>
                            <ListBox HorizontalAlignment="Center" Foreground="White" Name="lstPckdiv" Background="#363636" Grid.Column="0" BorderThickness="0"></ListBox>
                            <ListBox HorizontalAlignment="Center" Foreground="White" Name="lstPckprojectname" Background="#363636" Grid.Column="1" BorderThickness="0"></ListBox>
                            <ListBox HorizontalAlignment="Center" Foreground="White" Name="lstPckjrnr" Background="#363636" Grid.Column="2" BorderThickness="0"></ListBox>
