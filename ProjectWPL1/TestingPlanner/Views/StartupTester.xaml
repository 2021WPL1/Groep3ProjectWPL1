<Window x:Class="TestingPlanner.Views.StartupTester"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestingPlanner.Views"
        mc:Ignorable="d"
        Title="StartupTester" Height="450" Width="800">
    <StackPanel>
        <Image Style="{StaticResource BarcoLogo}"/>
        <StackPanel Margin="10">
            <GroupBox Header="Startscreen Testers" BorderThickness="0.1">
                <TextBlock Text="This is the startup screen for testers"/>
            </GroupBox>
        </StackPanel>
    </StackPanel>
</Window>
