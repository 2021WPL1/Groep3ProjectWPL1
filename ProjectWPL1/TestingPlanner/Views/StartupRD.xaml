<Window x:Class="TestingPlanner.Views.StartupRD"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestingPlanner.Views"
        mc:Ignorable="d"
        Height="720" Width="980" Background="#363636" Foreground="White"
        ResizeMode="NoResize"
        WindowState="Normal"
        WindowStyle="ThreeDBorderWindow"
        WindowStartupLocation="CenterScreen">
    <StackPanel>
        <Image Style="{StaticResource BarcoLogo}"/>
        <StackPanel Margin="10">
            <GroupBox Header="Open Job Requests" BorderThickness="0.1">
                <ListBox Height="150"/>
            </GroupBox>
            <StackPanel Orientation="Horizontal" Margin="10">
                <Button Content="New JR"/>
                <Button Content="Open JR details"/>
            </StackPanel>
        </StackPanel>
    </StackPanel>
</Window>
