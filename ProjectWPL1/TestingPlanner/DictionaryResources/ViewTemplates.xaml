<!-- Kaat -->
<!-- Datatemplates to connect viewmodels to views -->
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:viewModels="clr-namespace:TestingPlanner.Viewmodels"
                    xmlns:views="clr-namespace:TestingPlanner.Views">
    <DataTemplate DataType="{x:Type viewModels:ViewmodelTemporarilyStartUp}">
        <views:RequestFormUserControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:ViewmodelRequestForm}">
        <views:RequestFormUserControl/>
    </DataTemplate>
</ResourceDictionary>